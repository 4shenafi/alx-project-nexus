[run]
source = apps
omit = 
    */migrations/*
    */venv/*
    */env/*
    */__pycache__/*
    */tests/*
    manage.py
    */settings/*
    */celery.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

show_missing = True
precision = 2
